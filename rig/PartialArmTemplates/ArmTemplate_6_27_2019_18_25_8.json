{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "rig"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeone')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "hiram to dw",
				"activities": [
					{
						"name": "etl",
						"description": "copy",
						"type": "Copy",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"typeProperties": {
							"source": {
								"type": "SqlServerSource"
							},
							"sink": {
								"type": "SqlSink",
								"writeBatchSize": 10000
							},
							"enableStaging": false,
							"dataIntegrationUnits": 0,
							"translator": {
								"type": "TabularTranslator",
								"columnMappings": {
									"dt": "dt",
									"n": "n"
								}
							}
						},
						"inputs": [
							{
								"referenceName": "localhost",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "dw",
								"type": "DatasetReference"
							}
						]
					}
				],
				"concurrency": 1
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/localhost')]",
				"[concat(variables('factoryId'), '/datasets/dw')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/dw')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "asdw",
				"linkedServiceName": {
					"referenceName": "dw",
					"type": "LinkedServiceReference"
				},
				"type": "AzureSqlTable",
				"structure": [
					{
						"name": "dt",
						"type": "DateTime"
					},
					{
						"name": "n",
						"type": "Int32"
					}
				],
				"typeProperties": {
					"tableName": "[[dbo].[rigone]"
				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/localhost')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "hiram wks",
				"linkedServiceName": {
					"referenceName": "hiram",
					"type": "LinkedServiceReference"
				},
				"type": "SqlServerTable",
				"structure": [
					{
						"name": "dt",
						"type": "DateTime"
					},
					{
						"name": "n",
						"type": "Int32"
					}
				],
				"typeProperties": {
					"tableName": "[[dbo].[one]"
				}
			}
		}
	]
}