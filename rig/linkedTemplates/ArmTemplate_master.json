{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rig"
		},
		"dw_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'dw'"
		},
		"hf0524_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'hf0524'"
		},
		"hf0524_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'hf0524'"
		},
		"hiram_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'hiram'"
		},
		"hiramdw_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'hiramdw'"
		},
		"hiscale_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'hiscale'"
		},
		"jldw_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'jldw'"
		},
		"jldw_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'jldw'"
		},
		"jlhiramlaptop_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'jlhiramlaptop'"
		},
		"prod1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'prod1'"
		},
		"sqlva4onabrnvvfrkw_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqlva4onabrnvvfrkw'"
		},
		"hf0524_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "UPCIC\\HFleitas"
		},
		"jldw_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "JLAMIRAMAR.com\\hfleitas"
		},
		"jlhiramlaptop_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\fleitasarts\\erp-data"
		},
		"jlhiramlaptop_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "JLAMIRAMAR.com\\hfleitas"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"dw_connectionString": {
						"value": "[parameters('dw_connectionString')]"
					},
					"hiramdw_connectionString": {
						"value": "[parameters('hiramdw_connectionString')]"
					},
					"hiscale_connectionString": {
						"value": "[parameters('hiscale_connectionString')]"
					},
					"sqlva4onabrnvvfrkw_connectionString": {
						"value": "[parameters('sqlva4onabrnvvfrkw_connectionString')]"
					},
					"hf0524_connectionString": {
						"value": "[parameters('hf0524_connectionString')]"
					},
					"hf0524_password": {
						"value": "[parameters('hf0524_password')]"
					},
					"hiram_connectionString": {
						"value": "[parameters('hiram_connectionString')]"
					},
					"jldw_connectionString": {
						"value": "[parameters('jldw_connectionString')]"
					},
					"jldw_password": {
						"value": "[parameters('jldw_password')]"
					},
					"hf0524_properties_typeProperties_userName": {
						"value": "[parameters('hf0524_properties_typeProperties_userName')]"
					},
					"jldw_properties_typeProperties_userName": {
						"value": "[parameters('jldw_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"jlhiramlaptop_password": {
						"value": "[parameters('jlhiramlaptop_password')]"
					},
					"prod1_connectionString": {
						"value": "[parameters('prod1_connectionString')]"
					},
					"jlhiramlaptop_properties_typeProperties_host": {
						"value": "[parameters('jlhiramlaptop_properties_typeProperties_host')]"
					},
					"jlhiramlaptop_properties_typeProperties_userId": {
						"value": "[parameters('jlhiramlaptop_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}