{
	"name": "pipeone",
	"properties": {
		"description": "hiram to dw",
		"activities": [
			{
				"name": "SQL_to_SQLAzure",
				"description": "Copy data from SQL server to SQL Azure",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "SqlSource"
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000
					},
					"enableStaging": false,
					"dataIntegrationUnits": 0
				},
				"inputs": [
					{
						"referenceName": "localhost",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "dw",
						"type": "DatasetReference"
					}
				]
			}
		],
		"concurrency": 1
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}